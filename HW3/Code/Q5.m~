syms q
e = 0.9;
W = 50000;
S = 32;
b = 17;
C_D_0 = 0.015;
AR = b^2/S;
rho10km = 0.4135;
T = 23500;
K = 1/(pi*e*AR);
rho = 1.225;
L_D_max = sqrt(1/(4*C_D_0*K));
Thrust = W/L_D_max;
V_L_D_max_10km = sqrt((2/rho10km)*sqrt(K/C_D_0)*W/S);
V_L_D_max = sqrt((2/rho)*sqrt(K/C_D_0)*W/S);
C_L_1_2_max = (3/4)*((1/(3*K*(C_D_0^3)))^0.25);
V_L_D_2_max_10km = V_L_D_max_10km *(3^0.25);
V_L_D_2_max = V_L_D_max*(3^0.25);
C_L_1_3_max = (1/4)*((1/(3*K*(C_D_0^(1/3))))^0.75);
V_L_D_3_max_10km = V_L_D_max_10km *((1/3)^0.25);
V_L_D_3_max = V_L_D_max*((1/3)^0.25);
V_min_P = ((3/4)*((W/S)^2)*(1/(rho^2))*(1/C_D_0)*(1/(pi*e*AR)))^0.25;
V_min_P_10km = ((3/4)*((W/S)^2)*(1/(rho10km^2))*(1/C_D_0)*(1/(pi*e*AR)))^0.25;
T_ma(rho10km/rho10km)^0.6*T;
